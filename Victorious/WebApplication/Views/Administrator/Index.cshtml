@model WebApplication.Models.Admin

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="Administrator">
    <h2>Admin Menu</h2>
    <div class="section">
        @{ Html.RenderPartial("Games", Model.LoadGameTypes()); }
    </div>
    <div class="section">
        @{ Html.RenderPartial("Platforms", Model.LoadPlatformTypes()); }
    </div>
    <div class="section">
        @{ Html.RenderPartial("Brackets", Model.LoadBracketTypes()); }
    </div>
</div>